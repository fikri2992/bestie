{
    "manifest_version": 3,
    "name": "My Chat Extension",
    "version": "1.0",
    "description": "A simple chat extension",
    "permissions": [
        "storage"
    ],
    "web_accessible_resources": [{
        "resources": ["icons/*"],
        "matches": ["<all_urls>"]
    }],
    "host_permissions": ["http://localhost:3000/*"], 
    "action": {
        "default_popup": "popup.html"
    }
}